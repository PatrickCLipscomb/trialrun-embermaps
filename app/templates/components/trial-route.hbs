    {{#if showFilteredLocations}}
    <form >
      {{#each sortedLocations as |location|}}
      <div class="well">
      <label for={{location.name}}>{{location.name}} -- <em>{{location.category}}</em></label>
      <input type="checkbox" onChange= {{action (action "valueHasChanged" location) value="target.checked"}}>
      </div>
      {{/each}}
    </form>
    {{/if}}
