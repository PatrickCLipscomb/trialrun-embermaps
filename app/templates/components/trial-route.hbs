    <button {{action "showRouteMap"}} class="btn btn-lg">Show Map</button>
    <button {{action 'hideAllOptions'}}>Hide All Locations</button>
    <button {{action 'showFilteredLocations'}}>See All Location</button>
    <button {{action 'showFilteredBars'}}>showFilteredBars</button>
